	<queries>
		<query name="PO Master">
			<source>
				<model/>
			</source>
			<selection>
				<dataItem aggregate="none" name="PR Number" rollupAggregate="none">
					<expression>[Finance Master].[Purchase Requests].[PR]</expression>
					<XMLAttributes>
						<XMLAttribute name="RS_dataType" output="no" value="3"/>
						<XMLAttribute name="RS_dataUsage" output="no" value="1"/>
					</XMLAttributes>
				</dataItem>
				<dataItem aggregate="none" name="PO" rollupAggregate="none">
					<expression>[Finance Master].[Purchase Requests].[PO]</expression>
					<XMLAttributes>
						<XMLAttribute name="RS_dataType" output="no" value="3"/>
						<XMLAttribute name="RS_dataUsage" output="no" value="1"/>
					</XMLAttributes>
				</dataItem>
				<dataItem aggregate="none" name="Requested Date" rollupAggregate="none">
					<expression>[Finance Master].[Purchase Requests].[Requested Date]</expression>
					<XMLAttributes>
						<XMLAttribute name="RS_dataType" output="no" value="7"/>
						<XMLAttribute name="RS_dataUsage" output="no" value="1"/>
					</XMLAttributes>
				</dataItem>
				<dataItem aggregate="none" name="Vendor ID" rollupAggregate="none">
					<expression>[Finance Master].[Purchase Requests].[Vendor ID]</expression>
					<XMLAttributes>
						<XMLAttribute name="RS_dataType" output="no" value="3"/>
						<XMLAttribute name="RS_dataUsage" output="no" value="0"/>
					</XMLAttributes>
				</dataItem>
				<dataItem aggregate="none" name="Vendor Name" rollupAggregate="none">
					<expression>[Finance Master].[Purchase Requests].[Vendor Name]</expression>
					<XMLAttributes>
						<XMLAttribute name="RS_dataType" output="no" value="3"/>
						<XMLAttribute name="RS_dataUsage" output="no" value="0"/>
					</XMLAttributes>
				</dataItem>
				<dataItem name="Arguments">
					<expression>#/* Table names, column names and values as it is in DB */#
&apos;&lt;&apos; || 

&apos;POP_PV_DTL&apos;
|| &apos; &apos; || &apos;POP_PR_NO&apos; || &apos;=\&quot;&apos; || [PR Number] || &apos;\&quot;&apos; 

|| &apos;/&gt;&apos;
</expression>
				</dataItem>
				<dataItem name="Environment">
					<expression>#sq($account.parameters.ActiveEnvironment)#</expression>
				</dataItem>
				<dataItem name="User">
					<expression>#sq($account.personalInfo.userName)#</expression>
				</dataItem>
				<dataItem name="Auth Token">
					<expression># sq(URLEncode($account.parameters.AuthToken)) #</expression>
				</dataItem>
			</selection>
			<detailFilters>
				<detailFilter>
					<filterExpression>[Requested Date] between ?Startdate? and ?EndDate?</filterExpression>
				</detailFilter>
				<detailFilter use="optional">
					<filterExpression>[PO] = ?PO?</filterExpression>
				</detailFilter>
				<detailFilter use="optional">
					<filterExpression>[PR Number] = ?PR?</filterExpression>
				</detailFilter>
				<detailFilter use="optional">
					<filterExpression>[Vendor ID] = ?PEID?</filterExpression>
				</detailFilter>
				<detailFilter use="optional">
					<filterExpression>[Vendor Name] = ?vname?</filterExpression>
				</detailFilter>
			</detailFilters>
		</query>
