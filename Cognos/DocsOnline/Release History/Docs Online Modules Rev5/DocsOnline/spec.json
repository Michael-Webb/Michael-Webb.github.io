{
  "name": "DocsOnline",
  "schemaVersion": "1.0",
  "extensions": [
    {
      "perspective": "common",
      "comment": "This zip simply contains JavaScript file to be used in Documents Online reports. It should not affect any perspective, other than to make the js file available.",
      "features": [
        {
          "id": "FetchControl"
        },
        {
            "id":"FetchFunctions"
        },
        {
          "id": "com.ibm.bi.common.media",
          "comment": "This is the ID for the Widgets panel. It will be the container for the images below.",
          "collectionItems": [
            {
              "containerId": "com.ibm.bi.common.media",
              "id": "Paperclip",
              "name": "Paperclip",
              "options": {
                "altText": "View Attached Document",
                "imageLink": "v1/ext/DocsOnline/images/Paperclip.png"
              }
            },
            {
              "containerId": "com.ibm.bi.common.media",
              "id": "Clock",
              "name": "Clock",
              "options": {
                "altText": "Attachment Loading",
                "imageLink": "v1/ext/DocsOnline/images/Clock.png"
              }
            }
          ]
        }
      ]
    }
  ]
}
